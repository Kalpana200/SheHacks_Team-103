
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-b5kHyXgcpbZJO/tY9Ul7kGkf1S0CWuKcCD38l8YkeH8z8QjE0GmW1gYU5S9FOnJ0"
    crossorigin="anonymous"></script>
<script>
    function myFunction() {
        var element = document.body;
        element.classList.toggle("light-mode");

        if ($("nav").hasClass("nav-custom")) {

            $("nav").removeClass("navbar-dark");
            $("nav").removeClass("nav-custom");
            $("nav").addClass("navbar-light");
            $("nav").addClass("bg-light");
        }
        else {
            $("nav").removeClass("navbar-light");
            $("nav").removeClass("bg-light");
            $("nav").addClass("navbar-dark");
            $("nav").addClass("nav-custom");
        }

       if($("h1").hasClass("text-dm")){
        $("h1").removeClass("text-dm");
        $("h1").addClass("text-lm");
       }
       else{
        $("h1").addClass("text-dm");
        $("h1").removeClass("text-lm");
       }

       if($(".sirj-card").hasClass("custom-card-dm")){
        $(".sirj-card").removeClass("custom-card-dm");
        $(".sirj-card").addClass("custom-card-lm");
       }

       else{
        $(".sirj-card").removeClass("custom-card-lm");
        $(".sirj-card").addClass("custom-card-dm");
       }

       if($(".custom-btn").hasClass("btn-light")){
        $(".custom-btn").removeClass("btn-light");
        $(".custom-btn").addClass("btn-dark");
       }
        else{
            $(".custom-btn").removeClass("btn-dark");
            $(".custom-btn").addClass("btn-light");
        }

        if($("h3").hasClass("we-text-dm")){
        $("h3").removeClass("we-text-dm");
        $("h3").addClass("we-text-lm");
       }
       else{
        $("h3").addClass("we-text-dm");
        $("h3").removeClass("we-text-lm");
       }

       if($("h6").hasClass("quote-dm")){
        $("h6").removeClass("quote-dm");
        $("h6").addClass("quote-lm");
       }
       else{
        $("h6").addClass("quote-dm");
        $("h6").removeClass("quote-lm");
       }

       if($(".content").hasClass("content-dm")){
        $(".content").removeClass("content-dm");
        $(".content").addClass("content-lm");
       }
       else{
        $(".content").addClass("content-dm");
        $(".content").removeClass("content-lm");
       }

       if($(".logo").hasClass("logo-dm")){
        $(".logo").attr("src", "images/black.png");
       
        $(".logo").removeClass("logo-dm");
       }
       else{
        $(".logo").attr("src", "images/white.png");
        $(".logo").addClass("logo-dm");
       }
       if($("h5").hasClass("compose-dm")){
        $("h5").removeClass("compose-dm");
        $("h5").addClass("compose-lm");
       }
       else{
        $("h5").addClass("compose-dm");
        $("h5").removeClass("compose-lm");
       }
       

    }
</script>
<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script>
        AOS.init();
      </script>

</body>

</html>
